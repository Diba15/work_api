{
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "bun install && bun run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server"
        }
    ]
}