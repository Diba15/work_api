{
    "version": 2,
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "bunVersion": "1.x",
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "index.ts"
        }
    ]
}